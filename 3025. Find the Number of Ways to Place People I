class Solution {
    public int numberOfPairs(int[][] points) {
        Arrays.sort(points, (p1, p2) -> 
            p1[0] == p2[0] ? p2[1] - p1[1] : p1[0] - p2[0]);

        int pairCount = 0; 
        int n = points.length;
        final int INF = 1 << 30; 
        for (int i = 0; i < n; ++i) {
            int y1 = points[i][1];
            int maxY = -INF; 
            for (int j = i + 1; j < n; ++j) {
                int y2 = points[j][1];
                if (maxY < y2 && y2 <= y1) {
                    maxY = y2;
                    pairCount++;
                }
            }
        }

        return pairCount;
    }
}
