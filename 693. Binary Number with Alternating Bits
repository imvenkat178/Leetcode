class Solution {
    public boolean hasAlternatingBits(int n) {
        boolean f = true;
        int prev = -1;
        while(n!=0)
        {
            int curr = n&1;
            System.out.println(prev+" "+curr);
            if(prev == curr)
            {
                return false;
            }
            prev = curr;
            n = n>>1;
        }
        return true;        
    }
}
