class Solution {
    public boolean areAlmostEqual(String s1, String s2) {
        if(s1.equals(s2))
        {
            return true;
        }
        if(s1.length()!= s2.length())
        {
            return false;
        }
        //char[] char1 = s1.toCharArray();
        char[] char2 = s2.toCharArray();
        for(int i = 0 ; i < char2.length-1 ; i++)
        {
            for(int j = i+1 ; j < char2.length ; j++)
            {
                System.out.println(char2[i]+" "+char2[j]);
                char temp = char2[i];
                char2[i] = char2[j];
                char2[j] = temp;
                String ints2 = new String(char2);
                if(s1.equals(ints2))
                {
                    return true;
                }
                temp = char2[i];
                char2[i] = char2[j];
                char2[j] = temp;
            }
        }
        return false;
    }
}
