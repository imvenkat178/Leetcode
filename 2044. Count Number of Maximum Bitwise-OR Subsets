class Solution {
    public int countMaxOrSubsets(int[] nums) {
        int maxOR = 0;
        int[] count = new int[1];
        for(int i : nums)
        {
            maxOR |= i;
        }
        dfs(0,0,nums,maxOR,count);
        return count[0];
        
    }
    void dfs (int ind , int currOR ,int[] nums , int maxOR,int[] count)
    {
        if(ind == nums.length)
        {
            if(currOR == maxOR)
            {
                count[0]++;
            }
            return;
        }
        dfs(ind+1,currOR,nums,maxOR,count);
        dfs(ind+1,currOR|nums[ind],nums,maxOR,count);
    }
}
